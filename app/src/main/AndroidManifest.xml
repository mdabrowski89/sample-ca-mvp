<manifest package="pl.mobite.sample.ca.mvp"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:name=".MyApp"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:allowBackup="false">

        <activity android:name=".ui.components.home.HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity
            android:label="@string/users_list_title"
            android:name=".ui.components.userslist.UsersListActivity"/>

        <activity
            android:label="@string/users_list_paging_title"
            android:name="pl.mobite.sample.ca.mvp.ui.components.userlistpaging.UsersListPagingActivity"/>

    </application>
</manifest>
